{
    "package_version": 2,
    "version": "1.0.0",
    "name": "ui_tool",
    "description": "i18n:ui_tool.description",
    "main": "./dist/main.js",
    "devDependencies": {
        "@types/node": "^16.0.1",
        "typescript": "^4.3.4"
    },
    "author": "Cocos Creator",
    "editor": ">=3.7.3",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w"
    },
    "contributions": {
        "scene": {
            "script": "./dist/scene.js"
        },
        "menu": [{
            "path": "UI工具",
            "label": "自动绑定节点",
            "message": "binderNode"
        },
        {
            "path": "UI工具",
            "label": "自动生成代码",
            "message": "autoGenCode"
        },
        {
            "path": "UI工具",
            "label": "打印UI映射规则",
            "message": "printMappingRules"
        }],
        "messages": {
            "binderNode": {
                "methods": ["binderNode"]
            },
            "autoGenCode": {
                "methods": ["autoGenCode"]
            },
            "printMappingRules": {
                "methods": ["printMappingRules"]
            }
        },
        "shortcuts": [
            {
                "message": "binderNode",
                "win": "ctrl+b",
                "mac": "cmd+b"
            },
            {
                "message": "autoGenCode",
                "win": "ctrl+h",
                "mac": "cmd+h"
            }
        ]
    }
}
